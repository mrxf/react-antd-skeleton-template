    "@ksyun/antd-ksyun-theme": "0.0.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.165",
    "@types/node": "^12.0.0",
    "@types/react": "^16.14.2",
    "@types/react-dom": "^16.9.10",
    "@types/react-router-dom": "^5.1.6",
    "@types/url-join": "^4.0.0",
    "ahooks": "^2.9.2",
    "antd": "^4.9.1",
    "axios": "^0.21.0",
    "classnames": "^2.2.6",
    "craco-less": "^1.17.1",
    "echarts": "^5.0.0",
    "echarts-for-react": "^2.0.16",
    "env-cmd": "^10.1.0",
    "lodash": "^4.17.20",
    "path-to-regexp": "^6.2.0",
    "query-string": "^6.13.7",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "react-spring": "^9.0.0-rc.3",
    "swr": "^0.3.9",
    "typescript": "^4.1.2",
    "unstated-next": "^1.1.0",
    "url-join": "^4.0.1",
    "use-query-params": "^1.1.9",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "start": "craco start",
    "react-spring-issue-1078": "find node_modules -path \\*@react-spring/\\*/package.json -exec sed -i.bak 's/\"sideEffects\": false/\"sideEffects\": true/g' {} +",
    "postinstall": "npm run react-spring-issue-1078",
    "build": "craco build",
    "test": "craco test",
    "mock-server": "node_modules/.bin/json-server --delay 300 -p 4001 -r ./mock/routes.json -w ./mock/configs.js",
    "dev": "concurrently \"npm:start\" \"npm:mock-server\"",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/classnames": "^2.2.11",
    "concurrently": "^5.3.0",
    "http-proxy-middleware": "^1.0.6",
    "json-server": "^0.16.3",
    "mockjs": "^1.1.0"
  }
}
